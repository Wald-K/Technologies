        -insertbackground black 
    vTcl:DefineAlias "$top.ent45" "Entry1" vTcl:WidgetProc "Toplevel1" 1
    ###################
    # SETTING GEOMETRY
    ###################
    place $top.but43 \
        -in $top -x 190 -y 270 -anchor nw -bordermode ignore 
    place $top.ent45 \
        -in $top -x 180 -y 140 -anchor nw -bordermode ignore 

    vTcl:FireEvent $base <<Ready>>
}

